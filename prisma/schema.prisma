datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model VideoConvert {
  id          Int      @id @default(autoincrement())
  deviceId    String
  deviceToken String
  model       String?
  title       String?
  videoId     String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
