datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model VideoConvert {
  id          Int      @id @default(autoincrement())
  deviceId    String
  deviceToken String
  title       String?
  videoId     String
  version     String?
  input       Json?
  cancelUrl   String?
  status      String?
  iconUrl     String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
